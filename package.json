{
  "name": "glpk.js",
  "version": "5.0.0-pre.0",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "description": "GLPK for node & browser",
  "keywords": [
    "LP",
    "MILP",
    "solver",
    "linear programming",
    "linear optimization"
  ],
  "main": "dist/glpk.js",
  "browser": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:em && npm run build:js",
    "build:em": "make all",
    "build:js": "rollup -c rollup.config.js --worker && node src/pack.js && rollup -c rollup.config.js",
    "clean": "make clean",
    "test": "node test/test.js",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"test/**/*.js\" \"examples/**/*.js\" \"*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvail/glpk.js.git"
  },
  "author": "Jan Vaillant",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jvail/glpk.js/issues"
  },
  "homepage": "https://github.com/jvail/glpk.js#readme",
  "dependencies": {
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "almost-equal": "^1.1.0",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "rollup-plugin-copy": "^3.5.0",
    "tape": "^5.9.0"
  },
  "types": "dist/glpk.d.ts"
}
